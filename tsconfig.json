{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "include": ["src", "projects/botorch", "src/components/utils", "content/books", "content/personal", "projects/basictable", "projects/prompttable/components", "projects/prompttable/PromptTable.tsx", "projects/sidebysidetable"]
}
